{
	"info": {
		"_postman_id": "7d513f61-d888-4265-a3ff-2fa459e2667f",
		"name": "Sintegra Scraper API",
		"schema": "https://schema.getpostman.com/json/collection/v2.0.0/collection.json",
		"_exporter_id": "19411703"
	},
	"item": [
		{
			"name": "Webscrapear CNPJ",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"if (pm.response.code === 200) {",
							"    let jsonData;",
							"    try {",
							"        jsonData = pm.response.json();",
							"    } catch (e) {",
							"        return;",
							"    }",
							"    if (jsonData && Object.prototype.hasOwnProperty.call(jsonData, 'task_id')) {",
							"        pm.environment.set('task_id', jsonData.task_id);",
							"        pm.test(\"task_id is present and stored in environment\", function () {",
							"            pm.expect(jsonData.task_id).to.exist;",
							"        });",
							"    } else {",
							"        pm.test(\"task_id property not found in response\", function () {",
							"            pm.expect(jsonData.task_id).to.be.undefined;",
							"        });",
							"    }",
							"}"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"url": "{{API_URL}}/scrape"
			},
			"response": []
		},
		{
			"name": "Resultado de Task",
			"request": {
				"method": "GET",
				"header": [],
				"url": "{{API_URL}}/results/{{task_id}}"
			},
			"response": []
		}
	]
}